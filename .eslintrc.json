{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["xo", "prettier"],
  "overrides": [
    {
      "extends": ["xo-typescript", "prettier"],
      "files": ["*.ts", "*.tsx"],
      "rules": { "no-console": "error", "no-unused-vars": "warn" }
    },
    {
      "files": ["src/**/models/**/*.ts"],
      "rules": { "@typescript-eslint/naming-convention": "off" }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {}
}
